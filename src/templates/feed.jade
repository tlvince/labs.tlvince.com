doctype xml
rss(version='2.0',
    xmlns:content='http://purl.org/rss/1.0/modules/content/',
    xmlns:wfw='http://wellformedweb.org/CommentAPI/',
    xmlns:dc='http://purl.org/dc/elements/1.1/'
    xmlns:atom='http://www.w3.org/2005/Atom')
  channel
    - articles = contents.labs._.yaml
    title= locals.name
    atom:link(href=locals.url + '/feed.xml', rel='self', type='application/rss+xml')
    link= locals.url
    description= locals.description
    pubDate= new Date(articles[0].data.date)
    language en
    for article in articles
      item
        title= article.data.title
        link= article.data.url
        pubDate= new Date(article.data.date)
        author= locals.owner.name
        description= article.data.description
